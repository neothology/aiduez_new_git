{
    "input_features": [
        {
            "name": "Score",
            "type": "numerical",
            "encoder": "passthrough"
        },
        {
            "name": "Review",
            "type": "category",
            "encoder": "dense",
            "embedding_size": 50,
            "dropout": 0
        }
    ],
    "combiner": {
        "type": "concat"
    },
    "output_features": [],
    "preprocessing": {
        "numerical": {
            "normalization": "zscore"
        }
    },
    "training": {
        "epochs": 100,
        "batch_size": 128,
        "early_stop": 5,
        "optimizer": {
            "type": "adam"
        },
        "learning_rate": 0.001
    }
}